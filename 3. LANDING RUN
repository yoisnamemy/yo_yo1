function LANDINGRUN
clear all;
clc;
for To=70000:1000:80000
 g=9.81;
 mu=0.2;
 w1=17000:100:18000
 w=diag(w1)
 h=1000;
 lamda=6.5e-3
 to=288;
13
 t=to-(lamda*h);
 r=288;
 b=(g/((lamda*r)))-1;
 rho_o=1.225;
 rho=rho_o*(t/to)^b;
 s=40;
 cdg=0.02;
 clg=0.7;
 vtd=175;
a=0.65;
 A=g*(diag(diag((To*(inv(w))-mu))))
 B=g*inv(w)*(0.5*rho*s*(cdg+a-(mu*clg)))
 S=(1*(inv(2*B)))*log(1+((B*(vtd)^2)/A))
 S1=diag(S)
 plot(w1,S1)
 xlabel ('WEIGHT(W)')
 ylabel ('LANDING FIELD LENGTH(m)')
 title ('LANDING FIELD LENGTH V/S WWEIGHT')
 hold on
end
end
